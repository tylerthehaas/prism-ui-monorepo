{
  "name": "prism-ui",
  "version": "3.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Common UI for OC Tanner",
  "main": "dist/app.bundle.js",
  "module": "dist/app.bundle.js",
  "scripts": {
    "build": "rm -rf ./dist/ && NODE_ENV=production webpack -p",
    "build:library": "rm -rf ./dist/ && NODE_ENV=production webpack --config webpack.config.library.js",
    "dev:simple": "webpack-dev-server --port 3000",
    "dev": "webpack-dashboard -- webpack-dev-server --port 3000 --inline --progress",
    "start": "nodemon server.js",
    "lint": "eslint ./components --ext .js,.tsx",
    "prettier": "npm run prettier:js && npm run prettier:css",
    "prettier:js": "prettier --write \"./**/*.js\"",
    "prettier:css": "prettier --parser scss --write \"./**/*.scss\"",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook --config-dir .storybook",
    "test": "jest",
    "test:coverage": "npm run test --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "license": "ISC",
  "publishConfig": {
    "registry": "https://artifactory.octanner.net/api/npm/npmjs-repo/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octanner/prism-ui.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "dependencies": {
    "@storybook/addon-info": "^5.1.8",
    "@storybook/addon-knobs": "^5.1.8",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.1.9",
    "@storybook/source-loader": "^5.2.0-alpha.14",
    "@testing-library/react": "^8.0.4",
    "@types/webpack": "^4.4.34",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "core-js": "^2.6.9",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.18.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "jest-dom": "^2.1.1",
    "js-beautify": "^1.10.0",
    "prop-types": "^15.6.2",
