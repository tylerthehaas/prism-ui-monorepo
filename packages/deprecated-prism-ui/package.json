{
  "name": "@octanner/prism-ui",
  "version": "5.2.18",
  "engines": {
    "node": ">=12.8.1"
  },
  "description": "Common UI for OC Tanner",
  "main": "app.bundle.js",
  "module": "app.bundle.js",
  "scripts": {
    "bundle": "rm -rf ./dist && NODE_ENV=production webpack --config webpack.config.js",
    "css-minified": "NODE_ENV=production webpack --config webpack.config.css.minified.js && rm -rf ./css-only/app.bundle.js",
    "css-unminified": "NODE_ENV=production webpack --config webpack.config.css.unminified.js && rm -rf ./css-only/app.bundle.js",
    "copy": "cp ./package.json dist/package.json",
    "build": "npm run bundle && npm run css-minified && npm run css-unminified && npm run copy && tar -zcvf prism-ui.tar.gz dist && tar -zcvf prism-ui-css.tar.gz css-only && mv prism-ui-css.tar.gz ./css-only",
    "start": "nodemon server.js",
    "lint": "eslint ./components --ext .js,.ts,.tsx",
    "prettier": "npm run prettier:js && npm run prettier:css",
    "prettier:js": "prettier --write \"./**/*.js\"",
    "prettier:css": "prettier --parser scss --write \"./**/*.scss\"",
    "storybook": "start-storybook -s public",
    "build:storybook": "build-storybook -s public --config-dir .storybook",
    "test": "jest",
    "test:coverage": "npm run test --coverage",
    "test:watch": "yarn test --watch",
    "types": "index.d.ts",
    "publish:github": "npmrc github && npm publish prism-ui.tar.gz --registry=https://npm.pkg.github.com/",
    "prepublish": "rm -rf prism-ui.tar.gz && npm run build",
    "eslint-check": "eslint --print-config ./components/index.ts | eslint-config-prettier-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.jsx": [
      "yarn prettier:js",
      "yarn lint",
      "git add ."
    ],
    "*.css": [
      "yarn prettier:css",
      "git add ."
    ]
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/octanner/prism-ui.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "dependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/shortid": "0.0.29",
    "babel-plugin-require-context-hook": "^1.0.0",
    "node-fetch": "^2.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-focus-lock": "^2.0.5",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-docs": "^5.3.0-alpha.2",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storyshots": "^5.2.1",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.1",
    "@storybook/source-loader": "^5.2.0-alpha.14",
    "@testing-library/dom": "^5.6.1",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^8.0.9",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/webpack": "^4.39.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "24.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-transform-md-import-to-string": "^1.0.6",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "cors": "^2.8.5",
    "css-loader": "^1.0.1",
    "dotenv": "^7.0.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
