{
  "name": "prism-ui",
  "version": "1.0.20",
  "engines": {
    "node": ">=10.0.0 <11.0.0"
  },
  "description": "Common UI for OC Tanner",
  "main": "dist/app.bundle.js",
  "module": "dist/app.bundle.js",
  "scripts": {
    "build": "rm -rf ./dist/ && NODE_ENV=production webpack -p",
    "build:library": "rm -rf ./dist/ && NODE_ENV=production webpack --config webpack.config.library.js",
    "dev:simple": "webpack-dev-server --port 3000",
    "dev": "webpack-dashboard -- webpack-dev-server --port 3000 --inline --progress",
    "start": "nodemon server.js",
    "lint": "eslint . --ext .js",
    "prettier": "npm run prettier:js && npm run prettier:css",
    "prettier:js": "prettier --write \"./**/*.js\"",
    "prettier:css": "prettier --parser scss --write \"./**/*.scss\"",
    "test": "jest",
    "test:coverage": "npm run test --coverage",
    "test:watch": "npm run test --watch"
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "publishConfig": {
    "registry": "https://artifactory.octanner.net/api/npm/npmjs-repo/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octanner/prism-ui.git"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "core-js": "^3.1.3",
    "cors": "^2.8.5",
    "css-loader": "^1.0.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "helmet": "^3.16.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.0.10",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.13",
    "babel-jest": "24.5.0",
    "babel-plugin-ramda": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.6.1",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.5.0",
    "jest-dom": "^2.0.0",
    "jsdom": "^12.1.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "react-testing-library": "^5.1.1",
    "ts-jest": "^23.10.5",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2",
    "yarnhook": "^0.3.0"
  }
}
