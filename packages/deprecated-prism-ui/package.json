    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.1.9",
    "@testing-library/dom": "^5.4.0",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.13",
    "babel-eslint": "^10.0.2",
