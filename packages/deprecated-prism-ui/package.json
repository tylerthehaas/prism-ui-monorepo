    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-environment-jsdom-fifteen": "^1.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.14.3",
    "react-a11y": "^1.1.0",
    "react-axe": "^3.2.0",
    "react-docgen-typescript-loader": "^3.1.1",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "typescript": "^3.7.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.1"
  }
}
