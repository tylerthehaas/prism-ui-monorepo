{
  "name": "prism-ui",
  "version": "1.0.0",
  "description": "Common UI for OC Tanner",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors && ./node_modules/.bin/rollup -c",
    "build:css": "NODE_ENV=production webpack --progress --colors",
    "build:components": "./node_modules/.bin/rollup -c",
    "start": "node server",
    "test": "npm run build:components && BABEL_ENV=test ./node_modules/.bin/mocha --require jsdom-global/register --require babel-core/register \"./src/components/**/*.test.js\"",
    "report": "./node_modules/.bin/nyc report --reporter=text",
    "coverage": "BABEL_ENV=test ./node_modules/.bin/nyc --reporter=html ./node_modules/.bin/mocha --require babel-core/register \"./src/components/**/*.test.js\""
  },
  "license": "ISC",
  "publishConfig": {
    "registry": "https://artifactory.octanner.net/api/npm/npmjs-repo/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octanner/prism-ui.git"
  },
  "dependencies": {
    "jss": "^9.8.7",
    "jss-isolate": "^5.1.0",
    "jss-preset-default": "^4.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codacy-coverage": "^2.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "normalize.css": "^7.0.0",
    "nyc": "^12.0.2",
    "rollup": "^0.63.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.7.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": false,
    "report-dir": "./coverage/"
  }
}
