{
  "name": "@octanner/prism-ui",
  "version": "3.2.1",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "Common UI for OC Tanner",
  "main": "app.bundle.js",
  "module": "app.bundle.js",
  "scripts": {
    "bundle": "rm -rf ./dist && NODE_ENV=production webpack --config webpack.config.js",
    "css-minified": "NODE_ENV=production webpack --config webpack.config.css.minified.js && rm -rf ./css-only/app.bundle.js",
    "css-unminified": "NODE_ENV=production webpack --config webpack.config.css.unminified.js && rm -rf ./css-only/app.bundle.js",
    "copy": "cp ./package.json dist/package.json",
    "build": "npm run bundle && npm run css-minified && npm run css-unminified && npm run copy && tar -zcvf prism-ui.tar.gz dist && tar -zcvf prism-ui-css.tar.gz css-only && mv prism-ui-css.tar.gz ./css-only",
    "start": "nodemon server.js",
    "lint": "eslint ./components --ext .js,.tsx",
    "prettier": "npm run prettier:js && npm run prettier:css",
    "prettier:js": "prettier --write \"./**/*.js\"",
    "prettier:css": "prettier --parser scss --write \"./**/*.scss\"",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook --config-dir .storybook",
    "test": "jest",
    "test:coverage": "npm run test --coverage",
    "test:watch": "npm run test -- --watch",
    "publishArtifactory": "npmrc artifactory && npm publish prism-ui.tar.gz --registry https://artifactory.octanner.net/api/npm/oct-npmjs/",
    "publishGithub": "npmrc github && npm publish prism-ui.tar.gz --registry=https://npm.pkg.github.com/",
    "publish": "rm -rf prism-ui.tar.gz && npm run build && npm run publishArtifactory && npm run publishGithub"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/octanner/prism-ui.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "dependencies": {
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.32.2",
    "@types/yargs": "^13.0.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "core-js": "^2.6.9",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "helmet": "^3.20.0",
    "jest-dom": "^2.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-focus-lock": "^2.0.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.1.11",
    "@storybook/source-loader": "^5.2.0-alpha.14",
    "@testing-library/dom": "^5.6.1",
    "@testing-library/react": "^8.0.9",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "24.5.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express-static-gzip": "^1.1.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-environment-enzyme": "^7.1.0",
    "jsdom": "^12.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "react-a11y": "^1.1.0",
    "react-axe": "^3.2.0",
    "react-docgen-typescript-loader": "^3.1.1",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.2.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "typescript": "^3.5.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2",
    "yarnhook": "^0.3.0"
  }
}
