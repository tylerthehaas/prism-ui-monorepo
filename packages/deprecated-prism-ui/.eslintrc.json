{
  "env": { "jest/globals": true },
  "extends": [
    "airbnb-typescript",
    "airbnb/hooks",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier", "jest"],
  "rules": {
    "react/jsx-one-expression-per-line": "off",
    "react/prop-types": "off"
  }
}
